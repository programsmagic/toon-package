{
  "name": "toon-agent-bridge",
  "version": "0.1.0",
  "description": "Universal bridge for converting OpenAPI and agents.json schemas into agent-ready workflows with SSE/WebSocket streaming and React visualization",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "type-check": "pnpm -r type-check",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "prepare": "husky install",
    "build-docs": "node scripts/build-docs.js",
    "prepublishOnly": "pnpm build && pnpm build-docs"
  },
  "keywords": [
    "ai-agents",
    "openapi",
    "agents-json",
    "sse",
    "websocket",
    "react",
    "agent-bridge",
    "developer-tools"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.12.0"
}

