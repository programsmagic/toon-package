{
  "name": "@programsmagic/toon-converter",
  "version": "0.1.0",
  "description": "Multi-format converter (JSON/TOON/CSV/YAML) with smart format selection and auto-detection",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "jest"
  },
  "keywords": [
    "format-converter",
    "json",
    "toon",
    "csv",
    "yaml",
    "format-detection",
    "converter"
  ],
  "author": "ProgramsMagic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/programsmagic/toon-package.git",
    "directory": "packages/converter"
  },
  "bugs": {
    "url": "https://github.com/programsmagic/toon-package/issues"
  },
  "homepage": "https://programsmagic.github.io/toon-package",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@programsmagic/toon-format": "workspace:*",
    "@programsmagic/toon-core": "workspace:*",
    "js-yaml": "^4.1.0",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/js-yaml": "^4.0.9",
    "@types/papaparse": "^5.3.14",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  }
}

